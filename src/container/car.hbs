<Header><h1>Car List:</h1></Header>

<body>
  <main>
    <section>
      <div>
        <h2>Make: {{make}} {{model}}</h2>(year
        {{year}});
      </div>
    </section><hr />

    <section>
      <div>
        <h3>Features:</h3>
        <h4>Interior:</h4><br />

        <a
          class='btn btn-primary'
          data-bs-toggle='collapse'
          href='#collapseExample'
          role='button'
          aria-expanded='false'
          aria-controls='collapseExample'
        >
          Seats:
        </a>
        <div class='collapse' id='collapseExample'>
          <div class='card card-body'>
            {{#with features}}
              {{#with interior}}
                {{#with seats}}
                  Material:
                  {{material}}
                  <br />
                  Color:
                  {{color}}
                  <br />
                  Heated:
                  {{#if heated}}(yes)
                  {{else}}(no)
                  {{/if}}
                  Ventilated:
                  {{#if ventilated}}(yes)
                  {{else}}(no)
                  {{/if}}
                {{/with}}
              {{/with}}
            {{/with}}
          </div>
        </div>
      </div>
    </section>
    <section>
      <div>
        <a
          class='btn btn-primary'
          data-bs-toggle='collapse'
          href='#collapseExample'
          role='button'
          aria-expanded='false'
          aria-controls='collapseExample'
        >
          Dashboard:
        </a>
        <div class='collapse' id='collapseExample'>
          <div class='card card-body'>
            {{#with features}}
              {{#with interior}}
                {{#with dashboard}}
                  Material:
                  {{material}}
                  <br />
                  color:
                  {{color}}
                  <br />
                  Display:
                  {{#with display}}
                    <ul>
                      <li>Type: {{type}};</li>
                      <li>Size: {{size}};</li>
                      <li>Resolution: {{resolution}};</li>
                      <li>Touchscreen:
                        {{#if touchscreen}}(yes)
                        {{else}}(no)
                        {{/if}}
                      </li>
                    </ul>
                  {{/with}}
                {{/with}}
              {{/with}}
            {{/with}}
          </div>
        </div>
      </div>
    </section>

    <section>
      <a
        class='btn btn-primary'
        data-bs-toggle='collapse'
        href='#collapseExample'
        role='button'
        aria-expanded='false'
        aria-controls='collapseExample'
      >
        Audio:
      </a>
      <div class='collapse' id='collapseExample'>
        <div class='card card-body'>
          {{#with features}}
            {{#with interior}}
              {{#with audio}}
                <ul>
                  <li>System:
                    {{system}};</li>
                  <li>Speakers:
                    {{speakers}};</li>
                  <li>Subwoofer:
                    {{#if subwoofer}}(yes)
                    {{else}}(no)
                    {{/if}}
                  </li>
                  <li>Bluetooth:
                    {{#if bluetooth}}(yes)
                    {{else}}(no)
                    {{/if}}
                  </li>
                  <li>
                    USB:
                    {{#if USB}}(yes)
                    {{else}}(no)
                    {{/if}}
                  </li>
                </ul>
              {{/with}}
            {{/with}}
          {{/with}}
        </div>
      </div>
    </section><hr>

    <section>
      <div>
        <h4>Exterior:</h4><br />
      </div>
      <div>
        <a
          class='btn btn-primary'
          data-bs-toggle='collapse'
          href='#collapseExample'
          role='button'
          aria-expanded='false'
          aria-controls='collapseExample'
        >
          Wheels
        </a>
        <div class='collapse' id='collapseExample'>
          <div class='card card-body'>
            {{#with features}}
              {{#with exterior}}
                {{#with wheels}}
                  <ul>
                    <li>Size: {{size}};</li>
                    <li>Type: {{type}};</li>
                    <li>Color: {{color}};</li>
                  </ul>
                {{/with}}
              {{/with}}
            {{/with}}
          </div>
        </div>
      </div><br>

    <div>
         <a
          class='btn btn-primary'
          data-bs-toggle='collapse'
          href='#collapseExample'
          role='button'
          aria-expanded='false'
          aria-controls='collapseExample'
        >
          Headlights
        </a>
        <div class='collapse' id='collapseExample'>
          <div class='card card-body'>
            {{#with features}}
              {{#with exterior}}
                {{#with headlights}}
                  <ul>
                    <li>Type: {{type}};</li>
                    <li>Brightness: {{brightness}};</li>
                    <li>Automatic: {{#if automatic}}(yes)
                        {{else}}(no)
                        {{/if}};</li>
                  </ul>
                {{/with}}
              {{/with}}
            {{/with}}
          </div>
        </div>
      </div>
    </div><br>
        
    <div>
<a
          class='btn btn-primary'
          data-bs-toggle='collapse'
          href='#collapseExample'
          role='button'
          aria-expanded='false'
          aria-controls='collapseExample'
        >
          Sunroof
        </a>
        <div class='collapse' id='collapseExample'>
          <div class='card card-body'>
            {{#with features}}
              {{#with exterior}}
                {{#with sunroof}}
                  <ul>
                    <li>Type: {{type}};</li>
                    <li>size: {{size}};</li>
                    <li>Automatic: {{#if automatic}}(yes)
                        {{else}}(no)
                        {{/if}};</li>
                  </ul>
                {{/with}}
              {{/with}}
            {{/with}}
          </div>
        </div>
      </div>
    </div>
    </section><hr>

    <section>
        <div>
            <h4>Safety:</h4>
        </div><br>
<a
          class='btn btn-primary'
          data-bs-toggle='collapse'
          href='#collapseExample'
          role='button'
          aria-expanded='false'
          aria-controls='collapseExample'
        >
          Airbags
        </a>
        <div class='collapse' id='collapseExample'>
          <div class='card card-body'>
            {{#with features}}
              {{#with safety}}
                {{#with airbags}}
                  <ul>
                    <li>Front: {{front}};</li>
                    <li>Side: {{side}};</li>
                    <li>Knee: {{knee}};</li>
                    <li>Rear: {{rear}};</li>
                  </ul>
                {{/with}}
              {{/with}}
            {{/with}}
          </div>
        </div>      
       
        <div><br>
<a
          class='btn btn-primary'
          data-bs-toggle='collapse'
          href='#collapseExample'
          role='button'
          aria-expanded='false'
          aria-controls='collapseExample'
        >
          Assistance
        </a>
        <div class='collapse' id='collapseExample'>
          <div class='card card-body'>
            {{#with features}}
              {{#with safety}}
                {{#with assistance}}
                  <ul>
                    <li>Blind spot monitoring: {{#if blind_spot_monitoring}}(yes)
                        {{else}}(no)
                        {{/if}};</li>
                    <li>Rear cross traffic alert: {{#if rear_cross_traffic_alert}}(yes)
                        {{else}}(no)
                        {{/if}};</li>
                    <li>Lane departure warning: {{#if lane_departure_warning}}(yes)
                        {{else}}(no)
                        {{/if}};</li>
                    <li>Adaptive cruise control: {{#if adaptive_cruise_control}}(yes)
                        {{else}}(no)
                        {{/if}};</li>
                    <li>Collision warning: {{#if collision_warning}}(yes)
                        {{else}}(no)
                        {{/if}};</li>
                  </ul>                  
                {{/with}}
              {{/with}}
            {{/with}} 
          </div>
        </div>
        </div>
    </section><hr>

    <section>
        <div>
            <p>
  <button class="btn btn-primary" type="button" data-bs-toggle="collapse" data-bs-target="#collapseWidthExample" aria-expanded="false" aria-controls="collapseWidthExample">
    <h4>Engine:</h4>
  </button>
</p>
<div style="min-height: 120px;">
  <div class="collapse collapse-horizontal" id="collapseWidthExample">
    <div class="card card-body" style="width: 300px;">
        {{#with engine}}        
      <ul>
        <li>Type: {{type}}</li>
        <li>Displacement: {{displacement}}</li>
        <li>Horsepower: {{horsepower}}</li>
        <li>Torque: {{torque}}</li> 
        <li>Torque: {{torque}}</li>
            {{#with transmission}} <br>
                <b>Transmission:</b>
                <ul>
                    <li>Type: {{type}}</li>
                    <li>Gears: {{gears}}</li>
                </ul>
            {{/with}}       
      </ul>
      {{/with}}
    </div>
  </div><br><hr>
</div>
        </div> 
        
        <div>
            <h4>Fuel economy:</h4>
            {{#with fuel_economy}}
                City: {{city}} <br>
                Highway: {{highway}} <br>
                Combined: {{combined}} 
            {{/with}}
        </div><br>

        <div>
            <h4>Price:</h4>
                {{#with price}}
                <em>Base: {{base}};</em><br>
                <em>Destination: {{destination}};</em><br> 
                {{#with options}}
                <ul>                    
                <li><em>Navigation: {{navigation}};</em></li>
                <li><em>Moonroof: {{moonroof}};</em></li>
                <li><em>Premium paint: {{premium_paint}};</em></li>                 
                </ul><hr><br>                               
                {{/with}} 
                <h3>Total: {{total}}</h3>          
                {{/with}}
                
            
        </div>
    </section>
  </main>
</body>